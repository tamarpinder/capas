<svg xmlns="http://www.w3.org/2000/svg" width="800" height="400" viewBox="0 0 800 400">
  <defs>
    <linearGradient id="festivalGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#FF4500;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#FFD700;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#FF8B87;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="400" fill="url(#festivalGradient)"/>
  
  <!-- Junkanoo Masks -->
  <circle cx="200" cy="150" r="60" fill="#FFD700" stroke="#FF4500" stroke-width="4"/>
  <circle cx="180" cy="130" r="8" fill="#000"/>
  <circle cx="220" cy="130" r="8" fill="#000"/>
  
  <circle cx="400" cy="200" r="70" fill="#4169E1" stroke="#FFD700" stroke-width="4"/>
  <circle cx="380" cy="180" r="8" fill="#000"/>
  <circle cx="420" cy="180" r="8" fill="#000"/>
  
  <circle cx="600" cy="120" r="55" fill="#FF8B87" stroke="#4169E1" stroke-width="4"/>
  <circle cx="585" cy="105" r="8" fill="#000"/>
  <circle cx="615" cy="105" r="8" fill="#000"/>
  
  <!-- Feathers -->
  <ellipse cx="200" cy="90" rx="15" ry="40" fill="#FF4500" transform="rotate(-20 200 90)"/>
  <ellipse cx="220" cy="85" rx="15" ry="40" fill="#4169E1" transform="rotate(20 220 85)"/>
  
  <ellipse cx="400" cy="130" rx="15" ry="45" fill="#FFD700" transform="rotate(-10 400 130)"/>
  <ellipse cx="420" cy="125" rx="15" ry="45" fill="#FF8B87" transform="rotate(30 420 125)"/>
  
  <!-- Text -->
  <text x="400" y="320" text-anchor="middle" fill="#FFFFFF" font-size="28" font-weight="bold">Junkanoo Festival</text>
  <text x="400" y="350" text-anchor="middle" fill="#FFFFFF" font-size="16">Bahamian Cultural Celebration</text>
</svg>