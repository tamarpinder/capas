  <ellipse cx="200" cy="180" rx="15" ry="25" fill="#FFA07A"/>
  
  <!-- Palm Fronds -->
  <path d="M500 100 Q520 80 540 100 Q560 120 540 140 Q520 160 500 140 Q480 120 500 100" fill="#228B22"/>
  <path d="M520 90 Q540 70 560 90 Q580 110 560 130 Q540 150 520 130 Q500 110 520 90" fill="#228B22"/>
  
