  <rect x="600" y="140" width="100" height="20" fill="#FFD700"/>
  <polygon points="600,120 640,150 600,180" fill="#000"/>
  
  <!-- Traditional Pattern Border -->
  <path d="M0 350 Q50 330 100 350 T200 350 T300 350 T400 350 T500 350 T600 350 T700 350 T800 350" 
